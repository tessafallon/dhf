          <h2>My items</h2>
          <hr />
          <% if current_user.nil? %>
            <p><%= link_to 'Sign in', new_user_session_path %> to see recent contributions to the system.</p>
          <% else %>
            <% unless @my_items.empty? %>
              <dl class="activity-stream">
                <%= render partial: "modules/activity_stream_entry", collection: @my_items, as: :item %>
              </dl>
            <% else %>
              <p>You have not yet added anything to the system.</p>
            <% end %>
          <% end %>
